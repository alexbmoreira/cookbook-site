{
  "name": "Alexander's Famous Pumpkin Pie",
  "category": "dessert",
  "prepTime": 5,
  "cookTime": 50,
  "restTime": 120,
  "servings": 8,
  "ingredients": [
    {
      "quantity": "2",
      "measurement": "cups",
      "name": "pumpkin filling"
    },
    {
      "quantity": "1 3/4",
      "measurement": "cups",
      "name": "sweetened condensed milk"
    },
    {
      "quantity": "2",
      "measurement": "",
      "name": "eggs"
    },
    {
      "quantity": "1",
      "measurement": "tsp",
      "name": "cinnamon"
    },
    {
      "quantity": "1/2",
      "measurement": "tsp",
      "name": "nutmeg"
    },
    {
      "quantity": "1/2",
      "measurement": "tsp",
      "name": "ground ginger"
    },
    {
      "quantity": "1/2",
      "measurement": "tsp",
      "name": "salt"
    },
    {
      "quantity": "1",
      "measurement": "",
      "name": "(9-inch) unbaked pie crust"
    }
  ],
  "steps": [
    "Preheat oven to 425ºF.",
    "Whisk pumpkin, sweetened condensed milk, eggs, spices, and salt in medium bowl until smooth.",
    "Pour pupkin mixture into crust and bake for 15 minutes.",
    "Reduce oven temperature to 350ºF and continue baking 35 to 40 minutes or until toothpick inserted 1-inch from crust comes out clean.",
    "Let cool for 2 hours before serving."
  ]
}
